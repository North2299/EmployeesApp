        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="jp@gc - WebDriver Sampler" enabled="true">
          <stringProp name="WebDriverSampler.script">var pkg = JavaImporter(org.openqa.selenium, org.openqa.selenium.support.ui,org.openqa.selenium.support.events.EventFiringWebDriver);
var wait = new pkg.WebDriverWait(WDS.browser, 1500)// WebDriver wait

WDS.sampleResult.sampleStart(); //captures sampler&apos;s start time
WDS.sampleResult.getLatency();
WDS.log.info(&quot;Sample started&quot;);

// Launch website
WDS.browser.get(&quot;http://10.66.12.45:30170/&quot;);
WDS.log.info(&quot;Sample ended - navigated to http://10.66.12.45:30170/&quot;);

//resize the window
WDS.browser.manage().window().setSize(new pkg.Dimension(1552, 840));
WDS.browser.manage().window().setPosition(new pkg.Point(0, 0));

//add Employee button
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.xpath(&quot;/html/body/app-root/nav/div/ul/li/a&quot;)));
var addButton = WDS.browser.findElement(pkg.By.xpath(&quot;/html/body/app-root/nav/div/ul/li/a&quot;));
addButton.click();
WDS.log.info(&quot;add Employee button&quot;);

//enter name
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.id(&quot;name&quot;)));
var name = WDS.browser.findElement(pkg.By.id(&quot;name&quot;));
name.sendKeys(&quot;${uname}&quot;);
WDS.log.info(&quot;enter name3&quot;);

//enter idNumber
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.id(&quot;iDNumber&quot;)));
var iDNumber = WDS.browser.findElement(pkg.By.id(&quot;iDNumber&quot;));
iDNumber.sendKeys(&quot;${uid}&quot;);
WDS.log.info(&quot;enter idNumber&quot;);

//enter email
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.id(&quot;email&quot;)));
var email = WDS.browser.findElement(pkg.By.id(&quot;email&quot;));
email.sendKeys(&quot;${uemail}&quot;);
WDS.log.info(&quot;enter email&quot;);

//enter  jobTitle
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.id(&quot;jobTile&quot;)));
var jobTile = WDS.browser.findElement(pkg.By.id(&quot;jobTile&quot;));
jobTile.sendKeys(&quot;${ujob}&quot;);
WDS.log.info(&quot;enter jobTile&quot;);

//enter phone
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.id(&quot;phone&quot;)));
var phone = WDS.browser.findElement(pkg.By.id(&quot;phone&quot;));
phone.sendKeys(&quot;${uphone}&quot;);
WDS.log.info(&quot;enter phone&quot;);

//enter departement
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.id(&quot;departement&quot;)));
var departement = WDS.browser.findElement(pkg.By.id(&quot;departement&quot;));
departement.sendKeys(&quot;${udepartement}&quot;);
WDS.log.info(&quot;enter departement&quot;);

//submit
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.xpath(&quot;//button[@type=&apos;submit&apos;]&quot;)));
WDS.browser.findElement(pkg.By.xpath(&quot;//button[@type=&apos;submit&apos;]&quot;)).click();
WDS.sampleResult.sampleEnd()
</stringProp>
